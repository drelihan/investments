$("#line_item_row_<%= @row_number %>").hide(300, function() { 
	$('#revenue').html("<%= escape_javascript ( render :partial => 'line_items/line_items', :locals => { :type => "revenue", :line_items => @revenue_items } ) %>" );
	$('#expense').html("<%= escape_javascript ( render :partial => 'line_items/line_items', :locals => { :type => "expense", :line_items => @expense_items } ) %>" );
});
$("#results").html("Please reload");
